{
    "apps": [
        {
            "name": "rental-room-api",
            "script": "dist/main.js",
            "instances": 1,
            "exec_mode": "fork",
            "max_memory_restart": "512M",
            "env": {
                "NODE_ENV": "production"
            },
            "error_file": "/dev/null",
            "out_file": "/dev/null",
            "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
            "merge_logs": true,
            "autorestart": true,
            "max_restarts": 10,
            "min_uptime": "10s",
            "listen_timeout": 10000,
            "kill_timeout": 5000,
            "wait_ready": true,
            "shutdown_with_message": true
        }
    ]
}